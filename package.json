{
  "name": "inbank",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:INBANK": "node dist/INBANK/server/server.mjs",
    "dev:api": "ts-node server/index.ts",
    "build:api": "tsc -p tsconfig.server.json",
    "start:api": "node dist-server/index.js",
    "seed:api": "ts-node -P tsconfig.server.json server/seed.ts",
    "hash:pwd": "ts-node -P tsconfig.server.json server/util/hashPassword.ts",
    "dev:all": "npm run dev:api"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/platform-server": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@angular/ssr": "^20.2.2",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.6.1",
    "express": "5.1.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.1",
    "mysql2": "3.15.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.2",
    "@angular/cli": "^20.2.2",
    "@angular/compiler-cli": "^20.2.0",
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.19",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/morgan": "1.9.10",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "10.9.2",
    "typescript": "~5.9.2"
  }
}
